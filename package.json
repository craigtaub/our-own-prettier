{
  "name": "our-own-prettier",
  "version": "1.0.0",
  "description": "prettier vscode extension in 20 lines",
  "main": "./src/extension",
  "author": "craigtaub@gmail.com",
  "license": "ISC",
  "publisher": "craig-vscode-extension",
  "activationEvents": [
    "onCommand:extension.SexifyIt"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.SexifyIt",
        "title": "Sexify it"
      }
    ]
  },
  "configuration": {
    "type": "object",
    "title": "Craigs example configuration",
    "properties": {
      "languageServerExample.maxNumberOfProblems": {
        "scope": "resource",
        "type": "number",
        "default": 100,
        "description": "Controls the maximum number of problems produced by the server."
      }
    }
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "devDependencies": {
    "abstract-syntax-tree": "^2.8.1"
  }
}
